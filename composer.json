{
	"name": "causal/qr-redirects",
	"type": "typo3-cms-extension",
	"description": "This extension provides a Backend module to generate short URLs and QR codes leading to a given page.",
	"keywords": [
		"TYPO3 CMS",
		"QR codes",
		"Redirects",
		"Short URLs"
	],
	"homepage": "https://github.com/xperseguers/t3ext-qr-redirects",
	"support": {
		"issues": "https://github.com/xperseguers/t3ext-qr-redirects/issues"
	},
	"authors": [
		{
			"name": "Xavier Perseguers",
			"email": "xavier@causal.ch",
			"homepage": "https://www.causal.ch",
			"role": "Developer"
		}
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": ">=7.4.0 <=7.4.99 || >=8.0.0 <=8.2.99",
		"typo3/cms-core": "^10 || ^11 || ^12"
	},
	"autoload": {
		"psr-4": {
			"Causal\\QrRedirects\\": "Classes/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-main": "1.0.x-dev"
		},
		"typo3/cms": {
			"extension-key": "qr_redirects"
		}
	}
}
